        --el-color-primary-light-6: rgb(2, 53, 83);
        --el-color-primary-light-7: rgb(13, 44, 66);
        --el-color-primary-light-8: rgb(17, 35, 48);
        --el-color-primary-light-9: rgb(19, 26, 33);
