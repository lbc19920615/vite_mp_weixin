}

var defVal = function(v, defaultVal) {
  if (typeof v === 'undefined') {
    return defaultVal
  }
  return v
}

var isUnDef = function(v) {
  return typeof v === 'undefined'
}

/**
 * 
 * @param e 获取代码
 * @param instanse 
 */
function callEvent(e, instanse) {
  console.log(instanse.callMethod)
  instanse.callMethod('callEvent', {e: e})
}
module.exports = {
  foo: foo,
  bar: bar,
  isUnDef: isUnDef,
  defVal: defVal,
  callEvent: callEvent,
}